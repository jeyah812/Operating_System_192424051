#include <stdio.h>

int main() {
    int indexBlock, blocks[10], n;
    printf("Enter index block: ");
    scanf("%d", &indexBlock);
    printf("Enter number of blocks allocated: ");
    scanf("%d", &n);
    printf("Enter block numbers: ");
    for (int i = 0; i < n; i++) {
        scanf("%d", &blocks[i]);
    }

    printf("\nFile Indexed Allocation\n");
    printf("Index Block: %d\nBlocks: ", indexBlock);
    for (int i = 0; i < n; i++)
        printf("%d ", blocks[i]);
    printf("\n");
    return 0;
}
Index Block = 100  
Blocks = 105 106 107
File Indexed Allocation  
Index Block: 100  
Blocks: 105 106 107
